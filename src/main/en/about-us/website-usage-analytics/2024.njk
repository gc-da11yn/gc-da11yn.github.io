{% include "partials/analytics-toc.njk" %}

<div class="row">

{% for month, results in analytics2024 %}
	<section class="col-md-6 mrgn-tp-md mrgn-bttm-md">
		<div class="well">
			<h2 class="mrgn-tp-sm" id="{{ month }}">{{ month | formatYearMonth }}</h2>
			<dl>
				<dt>Visits:</dt>
				<dd>{{ results.results[0].sessions }}</dd>
				<dt>Users:</dt>
				<dd>{{ results.results[0].newUsers }}</dd>
				<dt>Pageviews:</dt>
				<dd>{{ results.results[0].screenPageViews }}</dd>
				<dt>Bounce Rate:</dt>
				<dd>{{ results.results[0].bounceRate }}%</dd>
				<dt>Average Session Duration:</dt>
				<dd>{{ results.results[0].avgSessionDuration }} minutes</dd>
				<dt>Most used devices:</dt>
				<dd>
					<ul>
						{% for device in results.results[0].topDevices %}
						<li>{{ device.device }} - {{ device.percentage }}%</li>
						{% endfor %}
					</ul>
				</dd>
				<dt>Top 3 countries:</dt>
				<dd>
					<ol>
						{% for country in results.results[0].topCountries %}
						<li>{{ country }}</li>
						{% endfor %}
					</ol>
				</dd>
			</dl>
		</div>
	</section>
{% endfor %}
</div>
